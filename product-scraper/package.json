{
  "name": "product-scraper-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter api dev\"",
    "build": "pnpm --filter @scraper/db build && pnpm --filter @scraper/scraper-core build && pnpm --filter api build",
    "start:prod": "pnpm --filter api start",
    "db:push": "pnpm --filter @scraper/db db:push",
    "db:studio": "pnpm --filter @scraper/db db:studio"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "concurrently": "^8.2.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.4.2",
    "prisma": "^5.14.0"
  },
  "dependencies": {
    "pg": "^8.16.3"
  }
}
